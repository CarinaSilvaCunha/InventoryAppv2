    <style name="scrollView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:fillViewport">true</item>
        <item name="android:orientation">vertical</item>
        <item name="android:scrollbars">none</item>
    </style>

    <style name="match_parent_linear_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/margin_ten</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="title_text">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">@dimen/title_text_size</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/info</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="row_linear_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/margin_ten</item>
        <item name="android:layout_weight">@integer/linear_layout_value_1</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="form_text_item">
        <item name="android:layout_width">@dimen/linear_zero</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_weight">@integer/linear_layout_value_1</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>

    <style name="edit_text">
        <item name="android:layout_width">@dimen/linear_zero</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_weight">@integer/linear_layout_value_2</item>
        <item name="android:inputType">text</item>
        <item name="android:maxLength">@integer/maxLength50</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:typeface">sans</item>
    </style>

    <style name="edit_text_alt">
        <item name="android:layout_width">@dimen/linear_zero</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_weight">@integer/linear_layout_value_3</item>
        <item name="android:maxLength">@integer/maxLength3</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:typeface">sans</item>
    </style>

    <style name="edit_book_genre">
        <item name="android:layout_width">@dimen/linear_zero</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">@integer/linear_layout_value_2</item>
        <item name="android:inputType">text</item>
        <item name="android:spinnerMode">dropdown</item>
    </style>

    <style name="image_button">
        <item name="android:layout_width">@dimen/linear_zero</item>
        <item name="android:layout_height">@dimen/linear_height</item>
        <item name="android:layout_weight">@dimen/image_linear</item>
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:contentDescription">@string/button</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="book_list">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:divider">@color/dividerColor</item>
        <item name="android:dividerHeight">@dimen/divider</item>
    </style>

    <style name="relative_layout_main">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="default_information">
        <item name="android:layout_centerInParent">true</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textSize">@dimen/title_text_size</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="floating_action_button">
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/margin_twenty</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:background">@color/color_special</item>
    </style>

    <style name="sell_book">
        <item name="android:layout_width">@dimen/linear_zero</item>
        <item name="android:layout_height">@dimen/image_height</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_weight">@integer/linear_layout_value_1</item>
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:contentDescription">@string/selling_icon</item>
        <item name="android:scaleType">fitCenter</item>
    </style>

    <style name="linear_layout_list">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/margin_ten</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:descendantFocusability">blocksDescendants</item>
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:maxHeight">@dimen/maxHeight</item>
    </style>

    <style name="book_info_list">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_margin">@dimen/margin_ten</item>
        <item name="android:layout_width">@dimen/linear_zero</item>
        <item name="android:layout_weight">@integer/linear_layout_value_3</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="book_name">
        <item name="android:layout_height">@dimen/linear_zero</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_weight">@integer/linear_layout_value_1</item>
    </style>

    <style name="book_price">
        <item name="android:layout_height">@dimen/linear_zero</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_weight">@integer/linear_layout_value_1</item>
    </style>

    <style name="books_in_stock">
        <item name="android:layout_height">@dimen/linear_zero</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_weight">@integer/linear_layout_value_1</item>
    </style>
</resources>
